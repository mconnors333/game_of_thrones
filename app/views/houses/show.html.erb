<h1><%= @house.name %></h1>
<%= link_to "Edit", edit_house_path(@house) %>
<%= link_to "Delete", @house, method: :delete, data: { confirm: 'Are you sure?' }%>
<% @house.characters.each do |character| %>
  <div class="character">
    <h2><%= link_to character.name, character_path(character) %></h2>
    <%= image_tag character.img_url %>
  </div>
<% end %>
